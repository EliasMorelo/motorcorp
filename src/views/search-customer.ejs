<!DOCTYPE html>
<html>
<%- include("partials/head") %>
<body>
	<div id="CONTAINER">
		<%- include("partials/navigation") %>
		<%- include("partials/messages") %>
		<i id="SIGNAL" class="fa fas fa-signal"></i>
		<i id="WIFI" class="fa fas fa-wifi"></i>
		<span id="HOUR">7:50 PM</span>
		<i id="BATTERY" class="fa fas fa-battery-full"></i>
		<div class="col-md-11">
			<h4 id="Search_Cos">Search</h4>
			<form action="/search-customer" method="post">
				<div class="input-group">
					<label Id="Label" for="Search_By">Search By:</label>
					<select name="by" id="Search_By">
						<option value="">Select an option</option>
						<option value="Name">Name</option>
						<option value="License Plate">License Plate</option>
						<option value="Vin">Vin</option>
					</select>
				</div>

				<div class="input-group">
					<div class="col-md-10">
						<input class="form-control" type="text" name="query" id="Query" required
							placeholder="Search here">
					</div>
					<input class="btn btn-primary" id="Button" type="submit" value="Search">

				</div>
			</form>
			<div class="form-group">
				<textarea name="show" id="Show" cols="46" rows="10"></textarea>
			</div>
		</div>
		<a href="/profile"><i Id="BACK_TWO" class="fa fas fa-arrow-circle-left"></i></a>
		<a href="/search-customer"><i Id="CIRCLE" class="fa fas fa-circle"></i></a>
	</div>
</body>
</html>
